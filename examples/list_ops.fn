# List operations example
# Demonstrates list creation, access, and List module functions

fn main():
    # Create a list
    let numbers = [10, 20, 30, 40, 50]

    # Get list length
    let len = List.len(numbers)
    println(len)

    # Access elements by index
    let first = List.get(numbers, 0)
    println(first)

    let third = List.get(numbers, 2)
    println(third)

    let last = List.get(numbers, 4)
    println(last)
